{
  "name": "spiceflow-website",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "pnpm --filter spiceflow build && pnpm readme && react-router build",
    "readme": "echo '{/* DO NOT EDIT: This file is auto-generated from root README.md */}' > ./app/routes/_mdx._index.mdx && cat ../README.md >> ./app/routes/_mdx._index.mdx",
    "deploy:prod": "pnpm run build && wrangler deploy ",
    "dev": "pnpm readme && react-router dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.13.4",
    "@cloudflare/workers-types": "^4.20250924.0",
    "@code-hike/mdx": "^0.9.0",
    "@mdx-js/react": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@react-router/cloudflare": "^7.9.1",
    "@react-router/fs-routes": "^7.9.1",
    "@stefanprobst/rehype-extract-toc": "^2.2.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/mdx": "^2.0.13",
    "isbot": "^4.1.0",
    "miniflare": "^4.20250508.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "^7.9.1",
    "rehype-slug": "^6.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^5.0.0",
    "spiceflow": "workspace:*",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240502.0",
    "@react-router/dev": "^7.9.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.2",
    "autoprefixer": "^10.4.19",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.38",
    "rehype-mdx-import-media": "^1.2.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^4.28.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
